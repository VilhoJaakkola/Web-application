  "description": "",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "check": "npm run format:check && npm run lint",
    "db:generate-types": "kysely-codegen --out-file=src/db/types.ts",
    "db:generate-types:watch": "kysely-codegen --out-file=src/db/types.ts --watch"
  },
  "dependencies": {
    "@trpc/server": "^11.7.1",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "kysely": "^0.28.8",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "superjson": "^2.2.6",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "@types/pino": "^7.0.4",
    "eslint": "^9.39.2",
    "kysely-codegen": "^0.19.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  }
